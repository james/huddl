<a href="/accounts/<%=account.id%>"><img <% if defined?(klass) %> class="<%=klass%>" <% end %> data-toggle="tooltip" title="<% if defined?(title) %><%=title%><% else %><%=account.name%><% end %> " <% if defined?(style) %> style="<%= style%>" <% else %> style="width: <%=width%>;" <% end %> src="<%=account.picture ? account.picture.thumb('400x400#').url : '/images/silhouette.png'%>" /></a>